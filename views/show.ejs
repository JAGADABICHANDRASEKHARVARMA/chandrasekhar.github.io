<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Experience</title>
      <!--  Bootstrap css file  -->
      <link rel="stylesheet" href="stylesheet/bootstrap.min.css">

      <!--  font awesome icons  -->
      <link rel="stylesheet" href="stylesheet/all.min.css">
  
  
      <!--  Magnific Popup css file  -->
      <link rel="stylesheet" href="vendor/Magnific-Popup/dist/magnific-popup.css">
  
  
      <!--  Owl-carousel css file  -->
      <link rel="stylesheet" href="vendor/owl-carousel/css/owl.carousel.min.css">
      <link rel="stylesheet" href="vendor/owl-carousel/css/owl.theme.default.min.css">

      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  
  
      <!--  custom css file  -->
      <link rel="stylesheet" href="stylesheet/style.css">
  
      <!--  Responsive css file  -->
      <link rel="stylesheet" href="stylesheet/responsive.css">
      <style>
        body{
            margin: 40px;
            border-radius: 25px;
            background: #9e90b8;
            padding: 15%;
            width: 200px;
            height: 150px;
        }
        #rcorners2 {
            border-radius: 25px;
            border: 2px solid black;
            padding: 20px; 
            width: 200px;
            height: 150px;  
            }
        .btn {
                background-color: rgb(112, 81, 221);
                border: none;
                color: white;
                padding: 12px 16px;
                font-size: 16px;
                cursor: pointer;
                border-radius: 25px;
}

/* Darker background on mouse-over */
.btn:hover {
  background-color: RoyalBlue;
}
#mongo{
    padding: 30%;
}
#zip{
    float: right;
}

      </style>
  
</head>

<body>
        

    <% sets.forEach(function(set){ %>
    <div id="rcorners2">
        <% set.set.forEach(function(item){ %>
        <%= item.name %>
        <% })%>
        <h2>
            <%= set.company %>
        </h2>
    </div>
    <% }) %>
    
        <div style="display: inline-block;" class="buttons" id="mongo">  
                    <a href="/" ><button class="btn"><i class="fa fa-home"></i> Home</button> </a>

                    <a href="/collect" ><button id="zip" class="btn"><i class="fa fa-close"></i> Close</button> </a>
     
        </div>
        
    
</body>

</html>